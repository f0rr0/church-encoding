{
  "name": "church-encoding",
  "version": "1.0.0",
  "description": "Church encoding primitives and data structures in javascript",
  "main": "dist/church.cjs.js",
  "module": "dist/church.esm.js",
  "browser": "dist/church.umd.js",
  "repository": "https://github.com/f0rr0/church-encoding",
  "author": "Sid Jain",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "codecov": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.15.0-rc.13",
    "jest": "^22.4.3",
    "jest-extended": "^0.6.0",
    "lint-staged": "^7.0.1",
    "np": "^2.20.1",
    "prettier": "1.11.1",
    "regenerator-runtime": "^0.11.1",
    "rollup": "^0.57.1"
  },
  "scripts": {
    "test": "jest",
    "test:ci": "jest --coverage && codecov",
    "format": "eslint --fix lib test",
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "test:watch": "jest --coverage --watchAll",
    "prepublishOnly": "yarn build",
    "release": "np"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "setupTestFrameworkScriptFile": "jest-extended",
    "collectCoverageFrom": [
      "lib/*.js",
      "!lib/index.js"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
